<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gui.TableViewUiController">
  <children>
    <VBox alignment="TOP_CENTER" prefHeight="423.0" prefWidth="341.0" AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="20.0">
      <children>
        <Label fx:id="TitleLbl" alignment="TOP_LEFT" text="ListView Example">
          <font>
            <Font name="System Bold" size="18.0" />
          </font>
        </Label>
            <TableView fx:id="tableViewMain" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="id" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="name" prefWidth="107.0" text="NAME" />
                  <TableColumn fx:id="surName" prefWidth="115.0" text="SURNAME" />
                  <TableColumn fx:id="salery" prefWidth="131.0" text="SALERY" />
              </columns>
            </TableView>
        <Label id="TextLbl" fx:id="LblAddText" text="Name" />
            <TextField fx:id="txtName" />
            <Label text="Surname" />
            <TextField fx:id="txtSurname" />
            <Label text="Salery" />
        <TextField fx:id="txtSalery" prefWidth="200.0" />
        <HBox id="HBox" alignment="CENTER" spacing="5.0">
          <children>
            <Button fx:id="BtnAdd" mnemonicParsing="false" onAction="#addAction" text="Add" />
            <Button fx:id="BtnDelete" mnemonicParsing="false" onAction="#deleteAction" text="Delete" />
          </children>
        </HBox>
      </children>
    </VBox>
  </children>
</AnchorPane>
